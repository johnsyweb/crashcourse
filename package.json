    "puppeteer": "^24.36.1",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.54.0",
    "vite": "^7.3.1"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5",
    "printWidth": 100,
    "tabWidth": 2,
    "semi": true
  },
  "packageManager": "pnpm@10.28.1+sha512.33097b555b3c2dc90c830207c14e7f3b8b14e0ea1ffc53b51ebbe82dbfad9071a71b6e20f75ac09f308a17cf6b08c084398d5bbe2d30b6e8a05e7f893f4e2e67"
