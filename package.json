  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc -b",
    "test": "NODE_OPTIONS='--no-warnings' jest",
    "check": "pnpm format:check && pnpm lint && pnpm typecheck && pnpm test",
